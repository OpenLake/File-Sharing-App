# Environment variables for File Sharing Application
# Copy this file to .env and update the values as needed

# MinIO Configuration
MINIO_ROOT_USER=minioadmin
MINIO_ROOT_PASSWORD=minioadmin123

# Backend Configuration
LOCAL_IP=minio:9000
ACCESS_KEY=minioadmin
SECRET_KEY=minioadmin123

# Frontend Configuration
API_BASE_URL=http://localhost:8000

# Optional: MinIO External Access (if you want to access MinIO console from outside Docker)
# MINIO_EXTERNAL_PORT=9001

# Optional: Custom Bucket Name (default: sarvesh)
# BUCKET_NAME=sarvesh